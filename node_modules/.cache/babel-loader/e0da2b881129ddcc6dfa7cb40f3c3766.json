,eAAA,GADA,CACA;AACA;AAxBA;AAdA,CAAA","sourcesContent":["<template>\n  <div>\n    <section\n      v-for=\"{ key, name, detail, place, member, money, year } in chat\"\n      :key=\"key\"\n      class=\"item\"\n    >\n      <div>\n        {{ name }}\n      </div>\n    </section>\n    <input type=\"text\" />\n    <input type=\"button\" value=\"更新\" @click=\"update()\" />\n  </div>\n</template>\n<script>\nimport { mapState, mapMutations } from \"vuex\";\nimport { COMPANYS_UPDATE, INIT } from \"../store/mutation-types\";\nimport firebase from \"firebase\";\n\nexport default {\n  data() {\n    return {\n      chat: [] // 取得したメッセージを入れる配列\n    };\n  },\n  created() {\n    this.INIT();\n  },\n  computed: {\n    ...mapState({\n      companys: \"companys\"\n    })\n  },\n  methods: {\n    ...mapMutations({\n      COMPANYS_UPDATE,\n      INIT\n    }),\n    scrollBottom() {\n      this.$nextTick(() => {\n        window.scrollTo(0, document.body.clientHeight);\n      });\n    },\n    childAdded(companys) {\n      this.chat.push({\n        key: companys.key,\n        name: companys.name,\n        detail: companys.detail,\n        place: companys.place,\n        member: companys.member,\n        money: companys.money,\n        year: companys.year\n      });\n      this.scrollBottom();\n    },\n    update() {\n      this.COMPANYS_UPDATE(); // mutationをコールします\n    }\n  }\n};\n</script>\n"],"sourceRoot":"src/views"}]}